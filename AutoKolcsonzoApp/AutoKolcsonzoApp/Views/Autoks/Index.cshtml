@model IEnumerable<AutoKolcsonzoApp.Models.Autok>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Marka)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rendszam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Szin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UlesekSzama)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Marka)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rendszam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Szin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UlesekSzama)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AutoID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AutoID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AutoID })
        </td>
    </tr>
}

</table>
